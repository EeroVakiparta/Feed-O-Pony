//Pin definitons
const int button1_pin = 2;
const int button2_pin = 3;
const int relay1_pin = 4;
const int relay2_pin = 5;
const int relay3_pin = 6;
const int relay4_pin = 7;

//Delays
int relayDelay = 500;
int buttonDelay = 1000;

//Inital default values
int feedingInterval = 1;
int feedingDelay = 1;

//Flag booleans
int timerOn = 0;
int refresh = 1;
int buttonPressed = 0;
int selectButton = 0;
int confirmButton = 0;

//Counters
int timerTick = 0;
int relayInTurn = relay1_pin; // the pin where the sequense starts
int relayCount = 4;

void setup() {
  Serial.begin(9600);
  // I/O defining
  pinMode(button1_pin, INPUT);
  pinMode(button2_pin, INPUT);
  pinMode(relay1_pin, OUTPUT);
  pinMode(relay2_pin, OUTPUT);
  pinMode(relay3_pin, OUTPUT);
  pinMode(relay4_pin, OUTPUT);

  //Screen setup
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();
  display.display();
  display.clearDisplay();
  display.setFont(&FreeSans9pt7b);
  display.setTextColor(WHITE);
  display.setCursor(0, 0);
  delay(1000);

  showStartingAnimation();
  digitalWrite(relay1_pin, HIGH);// (OFF)
  digitalWrite(relay2_pin, HIGH);// (OFF)
  digitalWrite(relay3_pin, HIGH);// (OFF)
  digitalWrite(relay4_pin, HIGH);// (OFF)
  delay(10);
  Serial.println("Feed-P-Pony ready!");
  startMenu();
}

void startMenu() {
  Serial.println("startMenu");
  display.clearDisplay();
  infoText();
  tuntiPiirto(feedingInterval);
  drawHay(relayCount);
}

void loop() {
  attachInterrupt(0, confirmButton, HIGH);
  buttonPressed = 0;
  if (refresh == 1) { //TODO: make prpper refresh a function
    Serial.println("Refresh screeni");
    display.clearDisplay();
    infoText();
    tuntiPiirto(feedingInterval);
    drawHay(relayCount - relayInTurn + relayCount);
    refresh = 0;
  }

  selectButton = digitalRead(button1_pin);
  confirmButton = digitalRead(button2_pin);

  // Kun valinta nappia painetaan ja timeri pois plt
  if (selectButton == HIGH && timerOn == 0 && buttonPressed == 0) {
    Serial.println("Valintanappia painettu");
    buttonPressed = 1;
    delay(buttonDelay);
    if (feedingInterval < 6) {
      feedingInterval = feedingInterval + 1;
    } else {
      feedingInterval = 1;
    }
    tuntiPiirto(feedingInterval);
  }
  // Kun start nappia painetaan ja timeri pois plt "timerin starttaus"
  if (timerOn == 0) {
    if (confirmButton == HIGH && buttonPressed == 0) {
      Serial.println("Conffianappia painettu");
      buttonPressed = 1;
      delay(buttonDelay);

      timerOn = 1;
      feedingDelay = feedingInterval * 10;
      relayInTurn = relay1_pin; // go back to first relay
      refresh = 1;
      started();
      delay(feedingDelay);
    }
  }
  // Timer looppi
  if (timerOn == 1) {
    // Jos timer on pll ja startti nappia painetaan timer stoppaa asiat nollaantuu
    if (confirmButton == HIGH && buttonPressed == 0) {
      Serial.println("Conffianappia painettu");
      buttonPressed = 1;
      delay(buttonDelay);
      timerOn = 0;
      feedingInterval = 1;
      timerTick = 0;
      refresh = 1;
      stopped();
    }
      LowPower.idle(SLEEP_8S, ADC_OFF, TIMER2_OFF, TIMER1_OFF, TIMER0_OFF, 
                SPI_OFF, USART0_OFF, TWI_OFF);

    timerTick = timerTick + 1;

    timerPiirto(timerTick);

    //TODO: write a relayfunction
    if (timerTick % feedingDelay == 0) {
      delay(relayDelay);
      digitalWrite(relayInTurn, LOW);// (ON)
      Serial.print("rele ");
      Serial.print(relayInTurn);
      Serial.println(" paalle ");
      delay(relayDelay);
      digitalWrite(relayInTurn, HIGH);//  (OFF)
      Serial.print("rele ");
      Serial.print(relayInTurn);
      Serial.println(" pois ");
      delay(relayDelay);
      relayInTurn = relayInTurn + 1;
      if (relayInTurn < relay1_pin + relayCount  ) {
        Serial.print("seuraavaksi rele: ");
        Serial.println(relayInTurn);
      } else {
        Serial.print("timer resetoitu");
        //TODO: make universalreset
        timerOn = 0;
        timerTick = 0;
        LowPower.powerDown(SLEEP_FOREVER, ADC_OFF, BOD_OFF); 
        detachInterrupt(0); 
      }
      drawHay(relayCount - relayInTurn + relayCount);
    }
  }
}
// Used to only update parts of the info screen by drawing black box on top of old values
void cleanaQuater(int q) {
  switch (q) {
    case 1: // top left
      display.fillRect(0, 0, display.width() / 2, display.height() / 2, SSD1306_BLACK);
      break;
    case 2: // top right
      display.fillRect(display.width() / 2, 0, display.width() / 2, display.height() / 2, SSD1306_BLACK);
      break;
    case 3: // bottom left
      display.fillRect(0, 18, display.width() / 2, display.height() / 2, SSD1306_BLACK);
      break;
    case 4: // bottom right
      display.fillRect(display.width() / 2, 18, display.width() / 2, display.height() / 2, SSD1306_BLACK);
      break;
    default:
      break;
  }
  display.display();
  delay(1);
}

void drawHay(int n) {
  cleanaQuater(4);
  display.setCursor(display.width() / 2, 32);
  for (int i = 0; i < n; i++) {
    display.print("# ");
    display.display();
  }
  delay(1);
}

void infoText(void) {
  cleanaQuater(1);
  display.setCursor(0, 15);     // x,y
  display.print("Ruoki:");
  display.display();
  delay(1);
}

void stopped(void) {
  display.clearDisplay();
  display.setCursor(16, 22);     // x,y
  display.print("STOPPED");
  display.display();
  delay(1000);
}

void started(void) {
  display.clearDisplay();
  display.setCursor(16, 22);     // x,y
  display.print("STARTED");
  display.display();
  delay(1000);
}

void tuntiPiirto(int h) {
  cleanaQuater(2);
  display.setCursor(70, 15);      // x,y
  display.print(h);
  display.print("0 s");
  display.display();
  delay(1);
}
void timerPiirto(int t) {
  cleanaQuater(3);
  display.setCursor(15, 31);      // x,y
  display.print(t);
  display.print(" s");
  display.display();
  delay(1);

}

//Iterates through the horseAnimation (x,y,frame,w,h,color)
void showStartingAnimation() {
  int frameDelay = 100;
  for (int i = 0; i < 20; i++) {
    display.clearDisplay();
    display.drawBitmap(0, 0, startAnim[i], display.width(), display.height(), 1);
    display.display();
    delay(frameDelay);
  }
}
